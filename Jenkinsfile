pipeline {
   agent any
   stages {
      stage('构建') {
         steps {
            echo 'Hello World'
         }
      }
      stage('审查') {
         steps {
            sleep 10
            echo '审查'
         }
       }
      stage('DevOps') {
         steps {
            echo 'ending........'
            echo 'end DevOps....'
         }
      }
   }
}
