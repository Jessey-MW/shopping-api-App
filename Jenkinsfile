pipeline {
    agent any
    stages {
       stage('代码提交') {
            steps {
              echo 'This is Newmaster2.........'
              sleep 20
            }
        }
        stage('代码审查') {
            steps {
               echo 'This is Newmaster2.........'
              sleep 10
            }
        }
        stage('Deploy结束') {
            steps {
               sleep 10
               sh 'ddddddd'
            }
        }
    }
}
